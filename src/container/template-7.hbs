<header class="header_all">
    {{#with header}}
        {{> headerDark}}
    {{/with}}
</header>

<main class="main">
    {{#with hero}}
    <h1>{{title}}</h1>
    <p>{{description}}</p>
    <a class="button" href="{{button.url}}">{{button.text}}</a>
    {{/with}}
</main>

<div class="about">
    {{#with about}}
    <h3>{{heading}}</h3>
    <p>{{info}}</p>
    {{/with}}
</div>

<div class="articles">
    <div class="articles_title">
        <h2>Our Articles</h2>
        <button>View All</button>
    </div>
    <div class="articles_card" id="{{id}}">
        {{#each articles}}
        {{> articleCardDark}}
        {{/each}}
    </div>

</div>

<footer class="footer" id="test">
    {{#with footer}}
    {{> footerDark}}
    {{/with}}
</footer>